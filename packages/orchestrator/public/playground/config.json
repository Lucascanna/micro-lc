{
  "$schema": "../../../../packages/interfaces/schemas/v2/config.schema.json",
  "version": 2,
  "settings": {
    "defaultUrl": "./",
    "composerUri": "/packages/orchestrator/dist/composer.development.js",
    "4xx": {
      "401": {
        "entry": "/packages/orchestrator/dist/401.html",
        "integrationMode": "parcel"
      },
      "404": {
        "entry": "/packages/orchestrator/dist/404.html",
        "integrationMode": "parcel"
      }
    },
    "5xx": {
      "500": {
        "entry": "/packages/orchestrator/dist/500.html",
        "integrationMode": "parcel"
      }
    }
  },
  "layout": {
    "sources": "/packages/layout/dist/mlc-config.js",
    "content": [
      {
        "tag": "mlc-config",
        "properties": {
          "initConfig": {
            "applications": {},
            "importmap": {},
            "layout": { "content": { "tag": "slot" } },
            "settings": {
              "composerUri": "http://localhost:8000/packages/orchestrator/dist/composer.production.js",
              "4xx": {
                "401": {
                  "entry": "http://localhost:8000/packages/orchestrator/dist/401.html",
                  "integrationMode": "parcel"
                },
                "404": {
                  "entry": "http://localhost:8000/packages/orchestrator/dist/404.html",
                  "integrationMode": "parcel"
                }
              },
              "5xx": {
                "500": {
                  "entry": "http://localhost:8000/packages/orchestrator/dist/500.html",
                  "integrationMode": "parcel"
                }
              },
              "mountPoint": [
                {
                  "content": "div#__MICRO_LC_MOUNT_POINT > :first-child {width: inherit; height: inherit; overflow: hidden;}",
                  "tag": "style"
                },
                {
                  "attributes": {
                    "id": "__MICRO_LC_MOUNT_POINT",
                    "style": "width: 100%; height: 100%;"
                  },
                  "tag": "div"
                }
              ],
              "mountPointSelector": "#__MICRO_LC_MOUNT_POINT"
            },
            "shared": {},
            "version": 2
          }
        }
      }
    ]
  },
  "applications": {
    "micro-lc": {
      "integrationMode": "iframe",
      "route": "./",
      "src": "./micro-lc.html"
    }
  }
}
