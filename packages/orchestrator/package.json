{
  "name": "@micro-lc/orchestrator",
  "version": "0.1.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "check:types": "tsc --project ./scripts/tsconfig.types.json && echo \"âœ“ check types\"",
    "prebuild": "ts-node scripts/pre-bundle.ts",
    "test": "wtr --node-resolve",
    "coverage": "yarn test:browser --coverage",
    "build:es": "ts-node scripts/compile.esm.ts",
    "build:cjs": "ts-node scripts/compile.cjs.ts",
    "build:prod": "vite build --mode production",
    "build:dev": "vite build --mode development",
    "build:prod:min": "vite build --mode production.min",
    "build:dev:min": "vite build --mode development.min",
    "build": "yarn run -T run-s prebuild build:es build:cjs build:dev build:prod",
    "cleanup": "yarn run -T rimraf ./dist ./.nyc_output ./coverage ./node_modules stats.html",
    "test:chromium": "wtr --node-resolve --playwright --browsers chromium",
    "test:firefox": "wtr --node-resolve --playwright --browsers firefox",
    "test:webkit": "wtr --node-resolve --playwright --browsers webkit",
    "esm": "ts-node scripts/esm-url-generator.ts",
    "serve": "wds --node-resolve"
  },
  "devDependencies": {
    "@babel/runtime": "^7.19.0",
    "@micro-lc/composer": "workspace:*",
