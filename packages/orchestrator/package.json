    "@types/sinon-chai": "^3.2.8",
    "@web/dev-server": "^0.1.34",
    "@web/dev-server-esbuild": "^0.3.2",
    "@web/dev-server-rollup": "^0.3.19",
    "@web/test-runner": "^0.14.0",
    "@web/test-runner-playwright": "^0.8.10",
    "ajv": "^8.11.0",
